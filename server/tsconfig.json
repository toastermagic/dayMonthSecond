{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true
  },
  "filesGlob":[
    "**/*.ts",
    "!typings/**/*"
  ]
}
